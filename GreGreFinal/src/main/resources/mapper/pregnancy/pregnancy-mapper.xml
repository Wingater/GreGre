<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pregnancy">

<select  id="findTable" resultType="_int">
select
	count(*)
from
	user_tables
where
	table_name = 'TB_MENSES_'||#{memberId}

</select>

<insert id="insertMenses">
insert into TB_MENSES_${memberId} values(seq_${memberId}_menses.currval,${startDay},default,${cycle},${nextMemses},${ageStart},${ageEnd},default)

</insert>
</mapper>